{
    "manifest_version": 2,
    "name": "Patio",
    "description": "A clean Speed Dial extension",
    "version": "0.1.0",
    "icons": {
        "16": "logo/logo_16.png",
        "48": "logo/logo_48.png",
        "128": "logo/logo_128.png",
        "512": "logo/logo_512.png"
    },
    "permissions": [
        "bookmarks",
        "history",
        "storage",
        "tabs",
        "*://*.google.com/*",
        "*://*.gstatic.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "background": {
        "scripts": [
            "worker/data.js",
            "worker/service-worker.js"
        ],
        "persistent": true 
    }
}
