{
    "manifest_version": 2,
    "default_locale": "en",
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "0.2.0",
    "browser_specific_settings": {
        "gecko": {
            "id": "@patio",
            "data_collection_permissions": {
                "required": [
                    "none"
                ]
            }
        }
    },
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png",
        "64": "icon/64.png"
    },
    "permissions": [
        "bookmarks",
        "history",
        "storage",
        "tabs",
        "*://*.google.com/*",
        "*://*.gstatic.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "chrome_settings_overrides": {
        "homepage": "newtab.html"
    },
    "background": {
        "scripts": [
            "worker/data.js",
            "worker/service-worker.js"
        ],
        "persistent": false
    }
}
