{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "0.2.0",
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png",
        "64": "icon/64.png"
    },
    "permissions": [
        "bookmarks",
        "history",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://*.google.com/*",
        "*://*.gstatic.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "background": {
        "service_worker": "worker/service-worker.js"
    }
}
