{
    "name": "Patio",
    "version": "0.2.0",
    "description": "A clean Speed Dial extension",
    "scripts": {
        "build": "npm run build:chrome",
        "release": "npm run release:chrome",
        "build:chrome": "vite build && cp ./manifest-chrome.json ./dist/manifest.json",
        "build:firefox": "vite build && cp ./manifest-firefox.json ./dist/manifest.json",
        "release:chrome": "vite build --sourcemap false && cp ./manifest-chrome.json ./dist/manifest.json",
        "release:firefox": "vite build --sourcemap false && cp ./manifest-firefox.json ./dist/manifest.json"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "lucide-react": "^0.555.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-icons": "^5.5.0",
        "sortablejs": "^1.15.6",
        "vite": "^6.3.4"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.16",
        "@types/chrome": "^0.1.24",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@types/sortablejs": "^1.15.9",
        "@vitejs/plugin-react": "^5.0.4",
        "daisyui": "^5.5.5",
        "tailwindcss": "^4.1.16"
    }
}
